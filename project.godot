config_version=5

[application]

config/name="GMTK24"
run/main_scene="res://Scenes/prototype_level.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=426
window/size/viewport_height=240
window/size/mode=2
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Assets/SFX/": "blue",
"res://Assets/Visuales/": "orange",
"res://Objects/": "pink",
"res://Scenes/": "purple"
}

[layer_names]

2d_physics/layer_1="Obstaculos"
2d_physics/layer_2="Ratas"
2d_physics/layer_3="big_Embudo "
2d_physics/layer_4="small_Embudo"
2d_physics/layer_5="big_widener"
2d_physics/layer_6="small_widener"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
